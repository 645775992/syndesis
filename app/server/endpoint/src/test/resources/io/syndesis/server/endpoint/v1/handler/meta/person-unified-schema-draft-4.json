{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "io:syndesis:webhook",
  "type": "object",
  "properties": {
    "parameters": {
      "type": "object",
      "required": ["version"],
      "properties": {
        "version": {
          "type": "string"
        }
      }
    },
    "body": {
      "type": "object",
      "$schema": "http://json-schema.org/schema#",
      "required": ["name", "age"],
      "properties": {
        "name": {
          "type": "string"
        },
        "age": {
          "type": "integer"
        }
      }
    }
  }
}
